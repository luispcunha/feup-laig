            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1" />
            <children>
                <primitiveref id="buildingWindow" />
            </children>
        </component>

        <component id="doorNode">
            <transformation>
                <translate x="10" y="0" z="0.05" />
            </transformation>
            <materials>
                <material id="whiteMaterial" />
            </materials>
            <texture id="doorTex" length_s="1" length_t="1" />
            <children>
                <primitiveref id="buildingDoor" />
            </children>
        </component>

        <component id="roofNode">
