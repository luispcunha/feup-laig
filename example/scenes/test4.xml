        </component>

        <component id="atomiumEdgePairNodeE">
            <transformation>
                <translate x="0" y="-3" z="0" />
                <rotate axis="x" angle="-90" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1" />
            <children>
                <componentref id="atomiumEdgePairNodeA" />
            </children>            
        </component>

        <component id="atomiumEdgePairNodeF">
            <transformation>
                <translate x="-3" y="-3" z="0" />
                <rotate axis="x" angle="-90" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1" />
            <children>
                <componentref id="atomiumEdgePairNodeA" />
            </children>            
        </component>


        <component id="atomiumEdgeNode">
            <transformation>
                <translate x="0" y="-3" z="0" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1" />
            <children>
                <primitiveref id="atomiumEdge" />
            </children>
        </component>


        <component id="houseNode">
            <transformation>
                <rotate axis="z" angle="180" /> 
                <rotate axis="x" angle="90" /> 
            </transformation>
            <materials>
                <material id="houseMaterial" />
            </materials>
            <texture id="woodenWallTex" length_s="3" length_t="3" />
            <children>
                <componentref id="wallNode1" />
                <componentref id="wallNode2" />
                <componentref id="wallNode3" />
                <componentref id="wallNode4" />
                <componentref id="roofNode" />
            </children>
        </component>

        <component id="wallNode1">
            <transformation>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1" />
            <children>
                <primitiveref id="buildingWall" />
                <componentref id="windowsNode" />
                <componentref id="doorNode" />
            </children>
        </component>

        <component id="wallNode2">
            <transformation>
                <transformationref id="wallTransf2" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1" />
            <children>
                <primitiveref id="buildingWall" />
            </children>
        </component>

        <component id="wallNode3">
            <transformation>
                <transformationref id="wallTransf3" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1" />
            <children>
                <primitiveref id="buildingWall" />
            </children>
        </component>

        <component id="wallNode4">
            <transformation>
                <transformationref id="wallTransf4" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1" />
            <children>
                <primitiveref id="buildingWall" />
            </children>
        </component>

        <component id="windowsNode">
            <transformation>
                <translate x="-2" y="2" z="0.05" />
            </transformation>
            <materials>
                <material id="whiteMaterial" />
            </materials>
            <texture id="windowTex" length_s="1" length_t="1" />
            <children>
                <componentref id="windowNode1" />
                <componentref id="windowNode2" />
            </children>
        </component>

        <component id="windowNode1">
            <transformation>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1" />
            <children>
                <primitiveref id="buildingWindow" />
            </children>
        </component>

        <component id="windowNode2">
