            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="beachBallTex" length_s="1" length_t="1" />
            <children>
                <primitiveref id="beachBall" />
            </children>
        </component>

        <component id="beachBallNode2">
            <transformation>
                <translate x="12" y="14" z="0.4" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="starsTex" length_s="1" length_t="1" />
            <children>
                <primitiveref id="beachBall" />
            </children>
        </component>

        <component id="mainPlaneNode">
            <transformation>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="mainPlaneTex" length_s="20" length_t="20" />
            <children>
                <primitiveref id="mainPlane" />
            </children>
        </component>


        <component id="parasolNode">
            <transformation>
                <translate x="0" y="0" z="0.05" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit" length_s="1" length_t="1" />
            <children>
                <componentref id="parasolTopNode" />
                <componentref id="parasolStickNode" />
                <componentref id="parasolBaseNode" />
            </children>
        </component>

        <component id="parasolStickNode">
            <transformation>
            </transformation>
            <materials>
                <material id="whiteMaterial" />
            </materials>
            <texture id="none" length_s="1" length_t="1" />
            <children>
                <primitiveref id="parasolStick" />
            </children>
        </component>

        <component id="parasolBaseNode">
            <transformation>
            </transformation>
            <materials>
                <material id="whiteMaterial" />
            </materials>
